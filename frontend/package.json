    "@tanstack/react-query-devtools": "^5.58.0",
    "@typescript-eslint/typescript-estree": "^8.8.1",
    "axios": "^1.7.7",
    "postcss-normalize": "^13.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
